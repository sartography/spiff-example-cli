    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_19d1ca2" sourceRef="StartEvent_1" targetRef="Activity_0gnvuqr" />
    <bpmn:userTask id="Activity_0gnvuqr" name="Select Product and Quantity" camunda:formKey="">
